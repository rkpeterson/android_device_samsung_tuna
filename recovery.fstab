# mount point   fstype  device

/system f2fs /dev/block/platform/omap/omap_hsmmc.0/by-name/system flags=fsflags="inline_xattr"
/cache f2fs /dev/block/platform/omap/omap_hsmmc.0/by-name/cache flags=fsflags="inline_xattr"
/data f2fs  /dev/block/platform/omap/omap_hsmmc.0/by-name/userdata flags=fsflags="inline_xattr"
/misc emmc /dev/block/platform/omap/omap_hsmmc.0/by-name/misc
/boot emmc /dev/block/platform/omap/omap_hsmmc.0/by-name/boot
/recovery emmc /dev/block/platform/omap/omap_hsmmc.0/by-name/recovery
/sbl emmc /dev/block/platform/omap/omap_hsmmc.0/by-name/sbl
/xloader emmc /dev/block/platform/omap/omap_hsmmc.0/by-name/xloader
/radio emmc /dev/block/platform/omap/omap_hsmmc.0/by-name/radio
/factory ext4 /dev/block/platform/omap/omap_hsmmc.0/by-name/efs flags=backup=1;display=EFS
/usb-otg vfat /dev/block/sda1 /dev/block/sda flags=removable;storage;wipeingui;display=USB-OTG
